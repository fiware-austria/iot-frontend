{
  "name": "iot-frontend",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Peter Salhofer",
  "description": "Angular Full Stack project built using Angular 6, Express, Mongoose and Node (based on Davide Violante's template)",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "buildprod": "ng build --aot=true --prod=true",
    "start": "node dist/server/server.js",
    "predev": "tsc -p server",
    "dev": "concurrently  \"ng serve --proxy-config proxy.conf.json --open\" \"tsc -w -p server\" \"nodemon dist/server/server.js\"",
    "prod": "concurrently \"ng build --aot=true --prod=true && tsc -p server && node dist/server/server.js\"",
    "test": "jest --watch --config=jest.config.json",
    "coverage": "jest --coverage --config=jest.config.json",
    "lint": "ng lint",
    "lintbe": "tslint server/**/**.ts{,x}",
    "e2e": "ng e2e",
    "test-srv": "jest --runInBand --watch",
    "coverage-srv": "jest --runInBand --coverage",
    "compile-tests": "tsc -w -p test"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^8.1.0",
    "@angular/compiler": "^8.1.0",
    "@angular/core": "^8.1.0",
    "@angular/forms": "^8.1.0",
    "@angular/http": "^7.2.15",
    "@angular/platform-browser": "^8.1.0",
    "@angular/platform-browser-dynamic": "^8.1.0",
    "@angular/router": "^8.1.0",
    "@auth0/angular-jwt": "2.1.1",
    "@types/mongoose": "^5.5.12",
    "@types/passport-jwt": "^3.0.1",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "babel-jest": "^24.8.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "bootstrap": "4.3.1",
    "config-yml": "^0.10.3",
    "core-js": "^2.6.9",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "font-awesome": "^4.7.0",
    "jest": "^24.8.0",
    "jquery": "^3.4.1",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.6.9",
    "mongoose-type-email": "^1.0.9",
    "morgan": "^1.9.1",
    "npm-check-updates": "^3.1.13",
    "passport": "^0.4.0",
    "passport-github": "^1.1.0",
    "passport-jwt": "^4.0.0",
    "rxjs": "^6.4.0",
    "socket.io": "^2.4.0",
    "socket.io-client": "^2.1.1",
    "superagent": "^5.1.0",
    "tether": "1.4.6",
    "typescript-logging": "^0.6.3",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.0",
    "@angular/cli": "8.1.0",
    "@angular/compiler-cli": "^8.1.0",
    "@angular/language-service": "^8.1.0",
    "@types/bluebird": "^3.5.23",
    "@types/jest": "^24.0.15",
    "@types/node": "~12.0.12",
    "@types/supertest": "^2.0.6",
    "bluebird": "^3.5.1",
    "codelyzer": "~5.1.0",
    "concurrently": "^4.1.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest-cli": "^24.8.0",
    "jest-preset-angular": "^7.1.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.3.1",
    "nodemon": "1.19.1",
    "protractor": "~5.4.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "^3.4.5"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(test/.*\\.(test|spec))\\.(tsx?|jsx?)$",
    "roots": [
      "test",
      "server"
    ]
  },
  "main": "dist/server/server.js"
}
